{
    "env": {
        "browser": true,
        "es6": true
    },
    "extends": [
        "airbnb-base"
    ],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "rules": {
        "no-unused-vars": "warn",
        "no-console": "off",
        "comma-dangle": "off",
        "react/jsx-filename-extension": "off",
        "indent": "off",
        "react/jsx-indent": "off",
        "react/jsx-indent-props": "off"
    }
}